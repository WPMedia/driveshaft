{
    "name": "driveshaft",
    "cluster": "alpine.nile.works",
    "image": "quay.io/washpost/driveshaft",
    "port": 3000,
    "routes": [
        {
            "paths": ["/"],
            "external": false
        }
    ],
    "builds": [
        {
            "notify": "driveshaft-dev", 
            "branch": "develop", 
            "version": "0.3.1", 
            "tags": ["latest"], 
            "autodeploy": true
        }
    ],
    "stages": [
        {
            "name": "dev",
            "scale": 2,
            "domain": "driveshaft.wpit.nile.works",
            "environment": {
                "GOOGLE_PRIVATE_KEY": "%{}"
            }
        }
    ]
}
